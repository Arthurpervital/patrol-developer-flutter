{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Cesla Debug",
      "type": "dart",
      "request": "launch",
      "cwd": ".",
      "flutterMode": "debug",
      "toolArgs": [
        "--dart-define-from-file",
        "env/env.json"
      ]
    },
    {
      "name": "Cesla Profile",
      "type": "dart",
      "request": "launch",
      "cwd": ".",
      "flutterMode": "profile",
      "toolArgs": [
        "--dart-define-from-file",
        "env/env.json"
      ]
    },
    {
      "name": "Cesla Release",
      "type": "dart",
      "request": "launch",
      "cwd": ".",
      "flutterMode": "release",
      "toolArgs": [
        "--dart-define-from-file",
        "env/env.json"
      ]
    },
    {
      "name": "attach debugger",
      "request": "attach",
      "type": "dart",
      "cwd": "integration_test",
      "vmServiceUri": "${command:dart.promptForVmService}"
    }
  ]
}